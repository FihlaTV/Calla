{
  "version": "1.0.0",
  "name": "calla-site",
  "private": true,
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "build:calla": "rollup -c --environment BUILD:calla",
    "build:yarrow": "rollup -c --environment BUILD:yarrow",
    "watch": "rollup -c -w --environment BUILD:development",
    "watch:calla": "rollup -c -w --environment BUILD:calla",
    "watch:yarrow": "rollup -c -w --environment BUILD:yarrow"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "tracekit": "^0.4.5"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch:yarrow"
    ]
  }
}
